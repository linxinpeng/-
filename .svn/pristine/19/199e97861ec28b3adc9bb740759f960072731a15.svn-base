body{
  background: #f9fafe;
}
.content{
  width: 1200px;
  margin: 10px auto;
}
.content .search{
  width: 100%;
  height: 59px;
  display: flex;
  align-items: center;
  background: #fff;
  position: relative;
  margin-bottom: 15px;
}
.content .search ul{
  padding-left: 28px;
}
.content .search ul li{
  float: left;
  font-size: 18px;
  color: #333333;
  margin-right: 66px;
  cursor: pointer;
}
.content .search ul li:hover{
  color: rgb(97,141,254);
}
.content .search form{
  position: absolute;
  top: 50%;
  right: 142px;
  transform: translateY(-50%);
  z-index: 1;
}
.content .search input{
  width: 400px;
  height: 38px;
  padding-left: 2px;
  border: none;
  outline: none;
  
  box-sizing: border-box;
}
.content .search img{
  position: absolute;
  top: 50%;
  right: 100px;
  transform: translateY(-50%);
  opacity: .2;
}

.c-content{
  width: 100%;
  height: 178px;
  box-sizing: border-box;
  padding: 10px;
  background: #ffffff;
  border-bottom: 1px solid #eee;
}
.content .i-page{
  width: 100%;
  height: 60px;
  padding-top: 26px;
  margin-bottom: 40px;
  background: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .i-page .go{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.content .i-page .go .g-s1{
  margin-left: 18px;
  font-size: 14px;
  color: #333333;
  margin-right: 10px;

}
.content .i-page .go .g-s2{
  margin-left: 7px;
  font-size: 14px;
  color: #333333;
}
.content .i-page .pagination li{
  display: inline-block;
  width: 28px;
  height: 28px;
  margin-right: 3px;
  text-align: center;
  line-height: 28px;
  border: 1px solid #ccc;
  cursor: pointer;
}
.content .i-page .pagination li a{
  display: inline-block;
  width: 100%;
  height: 100%;
}
.content .i-page .pagination .active{
  background: #467dfc;
  color: #fff;
}
.c-content .c-inner{
  display: flex;
  justify-content: space-between;
  text-align: left;
  height: 100%;
}
.c-content .c-inner img{
  width: 215px;
}
.c-content .c-inner .i-info{
  width: calc(100% - 250px);
}
.c-content .c-inner .i-info h3{
    font-size: 20px;
    color: #333333;
    font-weight: 600;
    margin-bottom: 25px;
    width: 90%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.c-content .c-inner .i-info p{
  font-size: 14px;
  line-height: 2;
  width: 80%;
  height: 56px;
  color: #999999;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  word-break: break-all;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}